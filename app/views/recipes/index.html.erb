<h1>My Recipes</h1>

<div class="row">
  <div class="col-md-8">
      <h3>Recipes </h3>
      <ol class="recipes">
        <% @recipes.each do |r| %>
        <a href=<%= r[:recipe][:link]%>><%= r[:recipe][:title] %><br></a>
        (<%= r[:percentage] %>% match!)
      
        <% end %>
      </ol>
    </div>
    <p>Scraped from <a href="https://www.hellofresh.com/recipes/quick-meals-collection">HelloFresh</a></p>
</div>
